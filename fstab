# <fs>                  <mountpoint>    <type>          <opts>                          <dump/pass>

/dev/sda1               /boot           ext2            noauto,noatime                  1 2
/dev/sda2               none            swap            sw                              0 0
/dev/sda3               /               reiser4         noatime,nodiratime,notail       0 1
/dev/hdb3               /home/ftp       xfs             noatime,nodiratime              0 0
/dev/hdb2               /media/exchange vfat            noauto                          0 0
/dev/mapper/vg-ftp      /mnt/ftp        xfs             noatime,nodiratime              0 0
/dev/sdb1               /home/ftp/data  reiser4         noatime,nodiratime              0 0

proc                    /proc           proc            defaults                        0 0

shm                     /dev/shm        tmpfs           nodev,nosuid,noexec             0 0
